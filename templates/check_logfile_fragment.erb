{
<% if (!@tag.nil?) -%>
tag => '<%=@tag%>',
<% end -%>
<% if (!@logfile.nil?) -%>
logfile => '<%=@logfile%>',
<% end -%>
<% if (!@criticalpatterns.nil?) -%>
criticalpatterns => [ <%=@criticalpatterns.sort.collect { | pattern | "'#{pattern}'" }.join(', ') %> ],
<% end -%>
<% if (!@criticalexceptions.nil?) -%>
criticalexceptions => [ <%=@criticalexceptions.sort.collect { | pattern | "'#{pattern}'" }.join(', ') %> ],
<% end -%>
<% if (!@warningpatterns.nil?) -%>
warningpatterns => [ <%=@warningpatterns.sort.collect { | pattern | "'#{pattern}'" }.join(', ') %> ],
<% end -%>
<% if (!@warningexceptions.nil?) -%>
warningexceptions => [ <%=@warningexceptions.sort.collect { | pattern | "'#{pattern}'" }.join(', ') %> ],
<% end -%>
<% if (!@okpatterns.nil?) -%>
okpatterns => [ <%=@okpatterns.sort.collect { | pattern | "'#{pattern}'" }.join(', ') %> ],
<% end -%>
<% if (!@rotation.nil?) -%>
rotation => '<%=@rotation%>',
<% end -%>
<% if (!@type.nil?) -%>
type => '<%=@type%>',
<% end -%>
<% if (!@options.nil?) -%>
options => '<%= @options.sort.collect { | opt,var | "#{opt}=#{var}" }.join(',') %>',
<% end -%>
},
